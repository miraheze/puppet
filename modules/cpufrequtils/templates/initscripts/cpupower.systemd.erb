[Unit]
Description=Apply cpupower configuration

[Service]
Type=oneshot
EnvironmentFile=-/etc/default/cpupower
ExecStart=/usr/libexec/cpupower
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
