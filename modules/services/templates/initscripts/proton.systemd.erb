[Unit]
Description=Mediawiki Proton Service
After=network.target

[Service]
User=proton
Group=proton
SyslogIdentifier=proton
WorkingDirectory=/srv/proton
Environment="CHROME_BIN=/usr/bin/chromium"
Environment="APP_ENABLE_CANCELLABLE_PROMISES=true"
ExecStart=/usr/bin/node /srv/proton/server.js -c /etc/mediawiki/proton/config.yaml
Restart=on-failure
PrivateTmp=true

[Install]
WantedBy=multi-user.target
