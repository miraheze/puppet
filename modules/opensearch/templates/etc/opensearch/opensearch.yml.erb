### MANAGED BY PUPPET ###
<%-
  $LOAD_PATH.unshift(File.join(File.dirname(__FILE__),"..","..","..","lib"))
  require 'puppet_x/opensearch/deep_to_i'
  require 'puppet_x/opensearch/hash'

  @yml_string = ''

  if !@data.empty?

    # Sort Hash and transform it into yaml
    @yml_string += Puppet_X::Opensearch::deep_to_i(
      @data
    ).extend(
      Puppet_X::Opensearch::SortedHash
    ).to_yaml
  end
-%>
<%= @yml_string %>
