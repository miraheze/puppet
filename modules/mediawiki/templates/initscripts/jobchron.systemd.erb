# Systemd job for Miraheze's JobChron config
# This file is managed by Puppet.
[Unit]
Description=Miraheze JobChron

[Service]
Type=simple
ExecStart=/srv/jobrunner/redisJobChronService --config-file=/srv/jobrunner/jobchron.json
User=www-data
StandardOutput=syslog
StandardError=syslog
SyslogIdentifier=jobchron
Restart=always

[Install]
WantedBy=multi-user.target
