# Note: This file is managed by puppet.

global
	maxconn 4096
	#debug
	#quiet
	user haproxy
	group haproxy
	stats socket <%= @socket %> mode 666 level user


defaults
	mode	http
	option	httplog
	option	dontlognull
	retries	3
	option redispatch
	maxconn	2000
	timeout connect 50000
	timeout client 500000
	timeout server 500000
