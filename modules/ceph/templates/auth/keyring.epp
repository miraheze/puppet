<%- |
  String[1] $client_name,
  String[1] $keydata,
  Ceph::Auth::Caps $caps,
| -%>
# Generated by puppet from modules/ceph/templates/keyring.epp
[<%= $client_name %>]
        key = <%= $keydata %>
<%# The capabilities are needed to be able to import them in case they don't exist -%>
<% $caps.each |String[1] $cap_name, String[1] $cap_value| { -%>
        caps <%= $cap_name %> = "<%= $cap_value %>"
<% } -%>
