# Consumed by openwebui.service
OLLAMA_BASE_URL=http://127.0.0.1:11434
OPENAI_API_KEY=<%= $backend_api_key %>
WEBUI_URL="https://ai.wikitide.net:443"
WEBUI_PORT=<%= $port %>
https_proxy=http://bastion.fsslc.wtnet:8080

# Customization
CUSTOM_NAME="WikiTide AI"

# Images
IMAGE_GENERATION_ENGINE="openai"
ENABLE_IMAGE_GENERATION="true"

# LDAP
ENABLE_LDAP="true"
ENABLE_LDAP_GROUP_MANAGEMENT="true"
ENABLE_LDAP_GROUP_CREATION="true"

LDAP_SERVER_LABEL="WikiTide LDAP"
LDAP_SERVER_HOST="ldap.wikitide.net"
LDAP_SERVER_PORT="636"
LDAP_USE_TLS="true"
LDAP_VALIDATE_CERT="true"

LDAP_APP_DN="cn=write-user,dc=miraheze,dc=org"

LDAP_SEARCH_BASE="dc=miraheze,dc=org"
LDAP_ATTRIBUTE_FOR_USERNAME="uid"
LDAP_ATTRIBUTE_FOR_MAIL="mail"
LDAP_SEARCH_FILTER="(uid=%s)"

# Web Search
ENABLE_WEB_SEARCH="true"
WEB_SEARCH_TRUST_ENV="true"
WEB_SEARCH_ENGINE="duckduckgo"